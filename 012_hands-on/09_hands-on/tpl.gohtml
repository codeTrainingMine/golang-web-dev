<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="main.css">
</head>
<body>

<table>
    <tr>
        <th>Date</th>
        <th>Open</th>
        <th>High</th>
        <th>Low</th>
        <th>Close</th>
        <th>Volume</th>
        <th>AdjClose</th>
    </tr>
    {{range .}}
        <tr>
            <td>
                {{.Date}}
            </td>
            <td>
                {{.Open}}
            </td>
            <td>
                {{.High}}
            </td>
            <td>
                {{.Low}}
            </td>
            <td>
                {{.Close}}
            </td>
            <td>
                {{.Volume}}
            </td>
            <td>
                {{.AdjClose}}
            </td>
        </tr>
    {{end}}

</table>

</body>
</html>